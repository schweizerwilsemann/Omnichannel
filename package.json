{
  "name": "omnichannel-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "be",
    "fe",
    "fe-customer"
  ],
  "scripts": {
    "install:be": "pnpm --filter ./be install",
    "install:fe": "pnpm --filter ./fe install",
    "install:fe-customer": "pnpm --filter ./fe-customer install",
    "start:fe-customer": "pnpm --filter ./fe-customer run start",
    "build:fe-customer": "pnpm --filter ./fe-customer run build",
    "dev": "pnpm -w --parallel --filter ./be run dev --filter ./fe run start --filter ./fe-customer run start",
    "start:be": "pnpm --filter ./be run start",
    "start:fe-administrator": "pnpm --filter ./fe-administrator run start",
    "build:fe-administrator": "pnpm --filter ./fe-administrator run build",
    "lint": "pnpm -w -r run lint",
    "test": "pnpm -w -r run test"
  }
}
